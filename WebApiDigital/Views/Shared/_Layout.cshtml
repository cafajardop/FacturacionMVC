<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/AngularNotification-Js.css" rel="stylesheet" />



</head>

<body ng-app="Generico">

    @RenderBody()

    <div class="container body-content">
        <hr />
        <footer>
            <p> &copy; @DateTime.Now.Year - Ing. Carlos Fajardo - Prueba Tecnica Digital Ware ASP.NET  </p>
        </footer>
    </div>

    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/app/UsuarioService.js"></script>
    <script src="~/Scripts/app/UsuarioController.js"></script>
    <script src="~/Scripts/app/app.js"></script>
    <script src="~/Scripts/AngularNotification-JS.js"></script>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>

</body>
</html>
